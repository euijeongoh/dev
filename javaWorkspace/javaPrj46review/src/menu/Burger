package menu;

public class Burger {

	
	public Burger() {
	}
	
	public Burger(String name, int price) {
		this.name = name;
		this.price = price;
	}

	private String name;
	private int price;
	
	@Override
	public String toString() {
		return name + "/" + price;  //여기서 코드 간단하게 고쳐주기
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getPrice() {
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
	
	
	
}//class
