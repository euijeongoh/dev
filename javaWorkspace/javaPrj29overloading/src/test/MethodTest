package test;

public class MethodTest {

	public void plusOne(int x) {
		System.out.println(x+1);
	}
	
	public void plus(int x, int y) {  //이런식으로 메소드의 이름을 동일하게 여러개 작성하는 것을 메소드 오버로딩이라고 한다.
		System.out.println(x+y);
	}
	
	public void plus(int x, int y, int z) { //매개변수 부분은 int같은 타입만 
		//파라미트 변수명은 상관 없고 타입 자체랑 순서를 신경써야함
		System.out.println(x+y);
	}
	
	public void plus(char a, int x, int y) {  //plus(int, int)이런것 만 아니면 ㄱㅊ
		System.out.println(x+y);
	}
	
	
	
	
	
}
