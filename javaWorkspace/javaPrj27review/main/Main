package main;

import data.Person;

public class Main {

	public static void main(String[] args) {

		System.out.println("복습!");
		
		Person p = new Person();
		p.setName("오의정"); //값 세팅 변수에 값을 채워넣을 수 있도록
		p.setAge (24);
		p.setGender ('F');
		
		String str = p.getFieldInfo();
		System.out.println(str);
		
	}//main

}//class
