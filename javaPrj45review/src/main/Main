package main;

import menu.Burger;

public class Main {

	public static void main(String[] args) {

		System.out.println("====맥도날드====");
		
		Burger b1 = new Burger("치즈버거", 5000);
//		b1.setName("치즈버거");
//		b1.setPrice(5000);
		
		String s = b1.getName();  //s라는 변수를 만들어서 버거의 이름에 넣어주기
		System.out.println(s);

		int p = b1.getPrice();  //p라는 변수를 만들어서 버거의 가격에 넣어주기
		System.out.println(p);
		
	}//main

}//class
